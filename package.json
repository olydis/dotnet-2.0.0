{
  "name": "dotnet-2.0.0",
  "version": "1.2.0",
  "description": "Platform agnostic installation of .NET Core Runtime 2.0.0",
  "engines": {
    "node": ">=6.4.0"
  },
  "main": "./dist/app.js",
  "typings": "./dist/main.d.ts",
  "bin": {
    "dotnet": "./dist/call.js",
    "dotnet-run": "./dist/call.js",
    "-dotnet": "./dist/app.js"
  },
  "scripts": {
    "publish-preview": "npm install && npm publish --tag preview",
    "build": "tsc",
    "prepare": "shx rm -rf ./dist/ && tsc && shx cp -R ./node_modules/fs-monkey/lib/ ./dist/fs-monkey/ && cd ./yarn && npm install",
    "start": "node dist/app.js",
    "postinstall": "node -e \"try { fs.statSync('./dist/app.js') ; require('./dist/app.js')} catch (e){}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olydis/dotnet-2.0.0.git"
  },
  "keywords": [
    "dotnet",
    ".NET",
    "core",
    "runtime",
    "2.0",
    "2.0.0",
    "install"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^8.0.22",
    "typescript": "^2.4.2",
    "fs-monkey": "~0.2.1",
    "shx": "~0.2.2"
  },
  "dependencies": {}
}
